<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.owner.anvilhackui" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity2Activity"
            android:label="@string/title_activity_main_activity2"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.owner.anvilhackui.MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity22Activity"
            android:label="@string/title_activity_main_activity22" >
        </activity>
        <activity
            android:name=".MacroPage"
            android:label="@string/title_activity_macro_page" >
        </activity>
        <activity
            android:name=".TrailRolls"
            android:label="@string/title_activity_trail_rolls" >
        </activity>
        <activity
            android:name=".TrialMacro"
            android:label="@string/title_activity_trial_macro" >
        </activity>
    </application>

</manifest>
